        <div class="row date-of-birth">
            <div class="col-sm-4 col-md-4">
                <div class="form-group">
                    <select name="month" class="form-control">
                        <option value="">Month</option>
                        <% (1..12).each do |month| %>
                            <option <%= 'selected' if params[:month].to_i == month %>><%= month %></option>
                        <% end %>
                    </select>
                </div>
            </div>
            <div class="col-sm-4 col-md-4">
                <div class="form-group">
                    <select name="day" class="form-control">
                        <option value="">Day</option>
                        <% (1..31).each do |day| %>
                            <option <%= 'selected' if params[:day].to_i == day %>><%= day %></option>
                        <% end %>
                    </select>
                </div>
            </div>
            <div class="col-sm-4 col-md-4">
                <div class="form-group">
                    <select name="year" class="form-control">
                        <option value="">Year</option>
                        <% (1900..Date.today.year).each do |year| %>
                            <option <%= 'selected' if params[:year].to_i == year %>><%= year %></option>
                        <% end %>
                    </select>
                </div>
            </div>
        </div>


